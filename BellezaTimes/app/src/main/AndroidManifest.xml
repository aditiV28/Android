<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.bellezatimes">

<uses-permission android:name="android.permission.INTERNET"></uses-permission>
<!--<uses-permission-sdk-23 android:name="android.permission.INTERNET"></uses-permission-sdk-23>-->
    <uses-feature
        android:glEsVersion="0x00030001"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity" />
        <activity android:name=".FeedPage" />
        <activity android:name=".ForgotPass" />
        <activity android:name=".ChangePass" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".DetailFeed"/>
        <activity android:name=".AdminFeedPage"/>
        <activity android:name=".InsertFeed"/>
        <activity android:name=".DeleteFeed"/>


        <!--
     ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>